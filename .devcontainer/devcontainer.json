{
  "name": "Kwot Web",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": ["dbaeumer.vscode-eslint", "esbenp.prettier-vscode"]
    }
  },
  "remoteUser": "me",
  "features": {
    "git-lfs": "latest",
    "git": {
      "version": "latest",
      "ppa": false
    },
    "github-cli": "latest",
    "ghcr.io/devcontainers/features/docker-from-docker:1": {
      "version": "latest"
    }
  }
}
